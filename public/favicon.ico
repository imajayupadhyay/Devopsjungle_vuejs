<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <linearGradient id="faviconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#047857;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#10b981;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background Circle -->
  <circle cx="16" cy="16" r="14" fill="url(#faviconGradient)" />

  <!-- Code Branch Icon -->
  <g transform="translate(16, 16)" fill="white">
    <!-- Main branch -->
    <circle cx="-5" cy="-3" r="1.5" />
    <circle cx="0" cy="0" r="1.5" />
    <circle cx="5" cy="3" r="1.5" />

    <!-- Branch lines -->
    <line x1="-3.5" y1="-3" x2="-1.5" y2="0" stroke="white" stroke-width="1" stroke-linecap="round"/>
    <line x1="1.5" y1="0" x2="3.5" y2="3" stroke="white" stroke-width="1" stroke-linecap="round"/>

    <!-- Secondary branch -->
    <circle cx="-5" cy="3" r="1.5" />
    <circle cx="5" cy="-3" r="1.5" />

    <!-- Secondary branch lines -->
    <line x1="-3.5" y1="3" x2="-1.5" y2="0" stroke="white" stroke-width="1" stroke-linecap="round"/>
    <line x1="1.5" y1="0" x2="3.5" y2="-3" stroke="white" stroke-width="1" stroke-linecap="round"/>
  </g>
</svg>